{
    "rust-analyzer.procMacro.enable": true,
    "[rust]": {
        "editor.tabSize": 4,
        "editor.detectIndentation": false,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": false
        }
    },
    "nixEnvSelector.suggestion": false,
    "nixEnvSelector.nixFile": "${workspaceRoot}/shell.nix"
}